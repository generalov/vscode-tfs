{
    "name": "vscode-tfs",
    "displayName": "TFS",
    "description": "TFS Visual Studio Code integration.",
    "version": "1.1.0",
    "author": {
        "name": "Evgeniy Generalov",
        "email": "e.generalov@gmail.com",
        "url": "https://github.com/generalov"
    },
    "contributors": [
        {
            "name": "Aaron Stevens",
            "email": "bheklilr2@gmail.com",
            "url": "https://github.com/bheklilr/vscode-tfs"
        }
    ],
    "publisher": "generalov",
    "license": "SEE LICENSE IN LICENSE",
    "keywords": [
        "tfs",
        "team",
        "foundation",
        "server",
        "microsoft"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/generalov/vscode-tfs.git"
    },
    "bugs": {
        "url": "https://github.com/generalov/vscode-tfs/issues"
    },
    "homepage": "https://github.com/generalov/vscode-tfs#readme",
    "readmeFilename": "README.md",
    "main": "./lib/extension",
    "engines": {
        "vscode": "^1.20.1"
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "mocha ./test/**/*.test.js",
        "lint": "eslint \"lib/**/*.js\" \"test/**/*.js\"",
        "fix": "prettier --write \"lib/**/*.js\" \"test/**/*.js\""
    },
    "activationEvents": [
        "*"
    ],
    "contributes": {
        "commands": [
            {
                "command": "vscode-tfs.menu",
                "title": "TFS Commands..."
            },
            {
                "command": "vscode-tfs.checkout",
                "title": "Checkout File",
                "category": "TFS"
            },
            {
                "command": "vscode-tfs.add",
                "title": "Add File",
                "category": "TFS"
            },
            {
                "command": "vscode-tfs.undo",
                "title": "Undo Changes",
                "category": "TFS"
            },
            {
                "command": "vscode-tfs.open-in-browser",
                "title": "Open in Browser",
                "category": "TFS"
            },
            {
                "command": "vscode-tfs.delete",
                "title": "Delete File",
                "category": "TFS"
            }
        ],
        "menus": {
            "commandPalette": [
                {
                    "command": "vscode-tfs.menu",
                    "when": "false"
                },
                {
                    "command": "vscode-tfs.checkout"
                },
                {
                    "command": "vscode-tfs.add"
                },
                {
                    "command": "vscode-tfs.undo"
                },
                {
                    "command": "vscode-tfs.open-in-browser"
                },
                {
                    "command": "vscode-tfs.delete"
                }
            ],
            "explorer/context": [
                {
                    "command": "vscode-tfs.menu"
                }
            ]
        },
        "configuration": {
            "type": "object",
            "title": "TFS configuration",
            "properties": {
                "tfs.location": {
                    "type": "string",
                    "description": "Path to the TFS command line client (tf.exe)."
                }
            }
        }
    },
    "icon": "res/icon.png",
    "galleryBanner": {
        "color": "#912a2a",
        "theme": "dark"
    },
    "categories": [
        "Other"
    ],
    "dependencies": {
        "child-process-promise": "^2.2.1"
    },
    "devDependencies": {
        "eslint": "6.0.1",
        "eslint-config-prettier": "6.0.0",
        "eslint-plugin-mocha": "5.3.0",
        "eslint-plugin-prettier": "3.1.0",
        "mocha": "^6.1.4",
        "prettier": "^1.18.2",
        "vscode": "^1.1.10"
    }
}
